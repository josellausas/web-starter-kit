{
  "name": "web-starter-kit",
  "version": "1.0.0",
  "main": "build/server.js",
  "repository": "https://github.com/josellausas/web-starter-kit.git",
  "author": "zunware <jose@josellausas.com>",
  "license": "MIT",
  "private": true,
  "scripts": {
    "server": "node build/server/server.js",
    "distribute": "npx webpack --config webpack.config.js",
    "build": "tsc --build",
    "build-server": "tsc -p server",
    "build-client": "tsc -p src",
    "debug": "node --inspect=9227 build/server/server.js",
    "lint": "eslint src/*.ts"
  },
  "quokka": {
    "babel": {
      "ts": true,
      "plugins": [
        "jsdom-quokka-plugin"
      ]
    }
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@babel/register": "^7.9.0",
    "@types/express": "^4.17.6",
    "@types/lodash": "^4.14.152",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@typescript-eslint/eslint-plugin": "^3.0.0",
    "@typescript-eslint/parser": "^3.0.0",
    "babel-loader": "^8.1.0",
    "eslint": "^7.0.0",
    "express": "^4.17.1",
    "jsdom-quokka-plugin": "^1.0.15",
    "lodash": "^4.17.15",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "source-map-loader": "^0.2.4",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.3",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "redux": "^4.0.5"
  }
}
